#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    int i=1;
      int row =1;
    while(i<=n){
         int column=1;
        while( column<=row)
        {
            cout<<row<<" ";
            
            column=column+1;
        }
        cout<<endl;
    
        row=row+1;
    }
    
  
}
